{
  "inbounds": [
    {
      "port": "$PORT",
      "protocol": "vless",
      "settings": {
        "clients": [{ "id": "$UUID" }],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "xhttp",
        "xhttpSettings": {
          "mode": "auto",
          "path": "$PATH"
        }
      }
    }
  ]
}
